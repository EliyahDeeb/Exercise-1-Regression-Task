   "source": [
    "# Compute AveragePrice if not present\n",
    "if 'AveragePrice' not in combined_df.columns:\n",
    "    if 'Low Price' in combined_df.columns and 'High Price' in combined_df.columns:\n",
    "        combined_df['AveragePrice'] = (combined_df['Low Price'] + combined_df['High Price']) / 2\n",
    "        print(\"Computed AveragePrice from Low Price and High Price\")\n",
    "    else:\n",
    "        raise ValueError(\"Cannot compute AveragePrice: 'Low Price' or 'High Price' missing.\")"
   ]
